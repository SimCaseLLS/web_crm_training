@model IEnumerable<TrainingCentersCRM.Models.Qualification>

@{
    ViewBag.Title = "Квалификации";
}
<div class="container row">
    <h2>Квалификации</h2>
    <div class="row">
        <div id="qualificationTree" style="width: 400px; overflow-x: scroll; white-space: nowrap; float: left"></div>
    </div>
    <div id="qualificationBlock" style="float: left">
        <div id="qualificationButtons" class="tools" style="display: none">
            <div style="display: inline">
                <button id="editQualification" class="button" title="Изменить">Изменить</button>
            </div>
            <div style="display: inline">
                <button id="deleteQualification" class="button" title="Удалить">Удалить</button>
            </div>
            <div style="display: inline">
                <button id="createQualification" class="button" title="Создать">Создать</button>
            </div>
        </div>
        <div id="qualificationData"></div>
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Scripts/QualificationPageSelect.js"></script>
    <script src="~/Scripts/createTree.js"></script>
    <script src="@Url.Content("~/Scripts/HeadHunterSelects.js")" type="text/javascript"></script>
}
